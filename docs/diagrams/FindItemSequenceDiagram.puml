@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":FoodNotesParser" as FoodNotesParser LOGIC_COLOR
participant "p:FindItemCommandParser" as FindItemCommandParser LOGIC_COLOR
participant "f:FindItemCommand" as FindItemCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant "predicate:ItemsContainKeywordsPredicate" as ItemsContainKeywordsPredicate MODEL_COLOR
end box
[-> LogicManager : execute(find-item chicken)
activate LogicManager

LogicManager -> FoodNotesParser : parseCommand(find-item chicken)
activate FoodNotesParser

create FindItemCommandParser
FoodNotesParser -> FindItemCommandParser :FindItemCommandParser()
activate FindItemCommandParser



FindItemCommandParser --> FoodNotesParser : p
deactivate FindItemCommandParser

FoodNotesParser -> FindItemCommandParser :parse(chicken)
activate FindItemCommandParser

create ItemsContainKeywordsPredicate
FindItemCommandParser -> ItemsContainKeywordsPredicate


activate ItemsContainKeywordsPredicate

ItemsContainKeywordsPredicate --> FindItemCommandParser : predicate

deactivate ItemsContainKeywordsPredicate
ItemsContainKeywordsPredicate -[hidden]-> FindItemCommandParser : result
destroy ItemsContainKeywordsPredicate

create FindItemCommand
FindItemCommandParser -> FindItemCommand : FindItemCommand(predicate)
activate FindItemCommand
FindItemCommand -> FindItemCommandParser : f
deactivate FindItemCommand

FindItemCommandParser --> FoodNotesParser: f
deactivate FindItemCommandParser
FindItemCommandParser -[hidden]-> FoodNotesParser : result
destroy FindItemCommandParser

FoodNotesParser --> LogicManager : f
deactivate FoodNotesParser

LogicManager -> FindItemCommand : execute()
activate FindItemCommand

FindItemCommand -> Model : updateFilteredStallList(chicken)
activate Model





Model --> FindItemCommand
deactivate Model

FindItemCommand --> LogicManager : result
deactivate FindItemCommand
FindItemCommand -[hidden]-> LogicManager : result
destroy FindItemCommand



[<--LogicManager
deactivate LogicManager
@enduml
